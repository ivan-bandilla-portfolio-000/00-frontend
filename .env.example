VITE_VENDOR_SCRIPT_URL=https://example.leadfeeder.com/snippet.js

# Enable fingerprinting via FingerprintJS (true/false)
VITE_ENABLE_FINGERPRINT=true

# Send an anonymized identify event to GA (true/false)
VITE_SEND_IDENTIFY_TO_GA=false

VITE_DISCORD_PROXY_URL=
VITE_DISCORD_PROXY_KEY=
