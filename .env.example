VITE_APP_ENV=local
VITE_APP_API_URL=localhost

VITE_PORTFOLIO_API_BASE=http://localhost:port

VITE_RECAPTCHA_SITE_KEY=


VITE_DATA_SOURCE_URL=

# Google Analytics 4 Measurement ID (format: G-XXXXXXXXX)
VITE_GA_ID=G-XXXXXXXXX

# Optional ipinfo token (note: client-side tokens are exposed publicly)
VITE_IPINFO_TOKEN=your_ipinfo_token_here

